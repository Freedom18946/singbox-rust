{
  "title": "SB Â· Proxy Selection",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Selections by pool/endpoint",
      "targets": [
        { "expr": "sum by (pool,endpoint) (rate(proxy_select_total[1m]))" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Endpoint RTT (EMA, ms)",
      "targets": [
        { "expr": "avg by (pool,endpoint) (proxy_endpoint_rtt_ms)" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Selection score",
      "targets": [
        { "expr": "avg by (pool,endpoint) (proxy_select_score)" }
      ]
    }
  ],
  "templating": {
    "list": [
      { "name": "pool", "query": "label_values(proxy_select_total, pool)", "type": "query" },
      { "name": "endpoint", "query": "label_values(proxy_select_total, endpoint)", "type": "query" }
    ]
  },
  "schemaVersion": 38
}