{
  "uid": "singbox-dns",
  "title": "SingBox-Rust Â· DNS",
  "description": "DNS query performance, error rates, and cache metrics for singbox-rust",
  "tags": ["singbox-rust", "dns"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "type": "row",
      "id": 1,
      "title": "DNS Query Metrics",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 2,
      "title": "Query Rate by Type (QPS)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (qtype) (rate(dns_query_total[1m]))",
          "legendFormat": "{{qtype}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean", "max"]
        },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "timeseries",
      "id": 3,
      "title": "DNS RTT Distribution (ms)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(dns_rtt_ms_bucket[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(dns_rtt_ms_bucket[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(dns_rtt_ms_bucket[5m])) by (le))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 0
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["last"]
        }
      }
    },
    {
      "type": "row",
      "id": 10,
      "title": "Error Tracking",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 11,
      "title": "Error Rate by Type",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (kind) (rate(dns_error_total[1m]))",
          "legendFormat": "{{kind}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "stat",
      "id": 12,
      "title": "Success Rate (%)",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 10 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(dns_success_total[5m])) / (sum(rate(dns_success_total[5m])) + sum(rate(dns_failed_total[5m])))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 90, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 13,
      "title": "Total Queries (1m)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 10 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(dns_query_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "stat",
      "id": 14,
      "title": "Total Errors (1m)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(dns_error_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 50, "color": "red" }
            ]
          },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "row",
      "id": 20,
      "title": "Cache Performance",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 21,
      "title": "Cache Hit Rate",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(dns_cache_hit_total[1m])) / (sum(rate(dns_cache_hit_total[1m])) + sum(rate(dns_cache_miss_total[1m])))",
          "legendFormat": "Hit Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "gauge",
      "id": 22,
      "title": "Cache Size",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 19 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "dns_cache_size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 5000, "color": "yellow" },
              { "value": 9000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "type": "stat",
      "id": 23,
      "title": "Requests from Cache",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 19 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(dns_success_total{from_cache=\"1\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "timeseries",
      "id": 24,
      "title": "Cache Operations",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 23 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "rate(dns_cache_hit_total[1m])",
          "legendFormat": "Hits/s",
          "refId": "A"
        },
        {
          "expr": "rate(dns_cache_miss_total[1m])",
          "legendFormat": "Misses/s",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "prometheus"
        }
      },
      {
        "name": "qtype",
        "type": "query",
        "query": "label_values(dns_query_total, qtype)",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "multi": true,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2
      }
    ]
  },
  "annotations": {
    "list": []
  }
}
