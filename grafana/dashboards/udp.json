{
  "uid": "singbox-udp",
  "title": "SingBox-Rust Â· UDP & NAT",
  "description": "UDP relay performance, NAT table monitoring, and session tracking for singbox-rust",
  "tags": ["singbox-rust", "udp", "nat"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "type": "row",
      "id": 1,
      "title": "UDP Traffic",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 2,
      "title": "Upstream Packet Rate (in)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "rate(udp_upstream_pkts_in_total[1m])",
          "legendFormat": "Packets In",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "pps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["last", "mean", "max"]
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "id": 3,
      "title": "Upstream Byte Rate (in)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "rate(udp_flow_bytes_in_total[1m])",
          "legendFormat": "Bytes In",
          "refId": "A"
        },
        {
          "expr": "rate(udp_flow_bytes_out_total[1m])",
          "legendFormat": "Bytes Out",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "stat",
      "id": 4,
      "title": "Total Bytes Transferred (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 9 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(udp_flow_bytes_in_total[1h])) + sum(increase(udp_flow_bytes_out_total[1h]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "color": { "mode": "value" },
          "decimals": 2
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 5,
      "title": "Total Packets (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 9 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(udp_upstream_pkts_in_total[1h]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "row",
      "id": 10,
      "title": "NAT Table",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "type": "gauge",
      "id": 11,
      "title": "NAT Map Size",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "udp_nat_size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 5000, "color": "yellow" },
              { "value": 9000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "type": "timeseries",
      "id": 12,
      "title": "NAT Map Size Over Time",
      "gridPos": { "h": 8, "w": 12, "x": 6, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "udp_nat_size",
          "legendFormat": "Sessions",
          "refId": "A"
        },
        {
          "expr": "udp_nat_heap_len",
          "legendFormat": "Heap Length",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["last", "mean", "max"]
        }
      }
    },
    {
      "type": "stat",
      "id": 13,
      "title": "Active Sessions",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "udp_upstream_map_size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "stat",
      "id": 14,
      "title": "Gen Mismatches",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 18 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "udp_nat_gen_mismatch_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 1000, "color": "red" }
            ]
          },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "row",
      "id": 20,
      "title": "NAT Eviction & TTL",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 21,
      "title": "Eviction Rate by Reason",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (reason) (rate(udp_nat_evicted_total[1m]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean", "max"]
        }
      }
    },
    {
      "type": "timeseries",
      "id": 22,
      "title": "Session TTL Distribution",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(udp_nat_ttl_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(udp_nat_ttl_seconds_bucket[5m])) by (le))",
          "legendFormat": "P90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(udp_nat_ttl_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "stat",
      "id": 23,
      "title": "Total Evictions (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 31 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(udp_nat_evicted_total[1h]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "row",
      "id": 30,
      "title": "Errors & Failures",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 35 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 31,
      "title": "Failure Rate by Class",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 36 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (class) (rate(udp_upstream_fail_total[1m]))",
          "legendFormat": "{{class}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "timeseries",
      "id": 32,
      "title": "Error Rate by Class",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 36 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (class) (rate(udp_upstream_error_total[1m]))",
          "legendFormat": "{{class}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "stat",
      "id": 33,
      "title": "Total Failures (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 44 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(udp_upstream_fail_total[1h]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 100, "color": "red" }
            ]
          },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "stat",
      "id": 34,
      "title": "Total Errors (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 44 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(udp_upstream_error_total[1h]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 100, "color": "red" }
            ]
          },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "prometheus"
        }
      }
    ]
  },
  "annotations": {
    "list": []
  }
}
