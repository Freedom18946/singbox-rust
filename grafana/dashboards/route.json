{
  "uid": "singbox-routing",
  "title": "SingBox-Rust · Routing & HTTP",
  "description": "HTTP response monitoring, routing metrics, and circuit breaker state for singbox-rust",
  "tags": ["singbox-rust", "routing", "http"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "type": "row",
      "id": 1,
      "title": "HTTP Metrics",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 2,
      "title": "HTTP Response Rate by Code",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (code) (rate(http_respond_total[1m]))",
          "legendFormat": "{{code}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean", "max"]
        },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "timeseries",
      "id": 3,
      "title": "HTTP Request Duration (Percentiles)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 0
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["last"]
        }
      }
    },
    {
      "type": "stat",
      "id": 4,
      "title": "Total Requests (1m)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 9 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(http_respond_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 5,
      "title": "Success Rate (2xx/3xx %)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 9 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(http_respond_total{code=~\"2..|3..\"}[5m])) / sum(rate(http_respond_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 90, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 6,
      "title": "Active Connections",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 9 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "http_active_connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 7,
      "title": "HTTP Errors (1m)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 9 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(http_errors_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 5, "color": "yellow" },
              { "value": 20, "color": "red" }
            ]
          },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "row",
      "id": 10,
      "title": "Error Tracking",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 11,
      "title": "HTTP Error Rate by Class",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (class) (rate(http_errors_total[1m]))",
          "legendFormat": "{{class}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "timeseries",
      "id": 12,
      "title": "4xx vs 5xx Responses",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(http_respond_total{code=~\"4..\"}[1m]))",
          "legendFormat": "4xx (Client Errors)",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_respond_total{code=~\"5..\"}[1m]))",
          "legendFormat": "5xx (Server Errors)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "row",
      "id": 20,
      "title": "Routing & Circuit Breaking",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 21,
      "title": "Route Explain Invocations",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "rate(route_explain_total[1m])",
          "legendFormat": "Explain Calls/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "id": 22,
      "title": "Circuit State Changes",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (pool, endpoint, state) (rate(proxy_circuit_state_total[1m]))",
          "legendFormat": "{{pool}}/{{endpoint}} → {{state}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last"]
        }
      }
    },
    {
      "type": "stat",
      "id": 23,
      "title": "Proxy Health Status",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 31 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "avg by (pool, endpoint) (proxy_up)",
          "legendFormat": "{{pool}}/{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "max": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          },
          "mappings": [
            { "type": "value", "value": 0, "text": "Down" },
            { "type": "value", "value": 1, "text": "Up" }
          ]
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background",
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "id": 24,
      "title": "Adapter Retries (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 31 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(adapter_retries_total[1h]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 100, "color": "red" }
            ]
          },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 25,
      "title": "Total Route Explains",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 31 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "route_explain_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "prometheus"
        }
      },
      {
        "name": "code",
        "type": "query",
        "query": "label_values(http_respond_total, code)",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "multi": true,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2
      }
    ]
  },
  "annotations": {
    "list": []
  }
}
