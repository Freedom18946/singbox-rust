{
  "uid": "singbox-overview",
  "title": "SingBox-Rust Â· Overview",
  "description": "Comprehensive overview of singbox-rust system health, performance, and key metrics",
  "tags": ["singbox-rust", "overview", "dashboard"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [
    {
      "title": "DNS Dashboard",
      "url": "/d/singbox-dns",
      "icon": "dashboard",
      "type": "link"
    },
    {
      "title": "Proxy Dashboard",
      "url": "/d/singbox-proxy",
      "icon": "dashboard",
      "type": "link"
    },
    {
      "title": "UDP Dashboard",
      "url": "/d/singbox-udp",
      "icon": "dashboard",
      "type": "link"
    },
    {
      "title": "Routing Dashboard",
      "url": "/d/singbox-routing",
      "icon": "dashboard",
      "type": "link"
    }
  ],
  "panels": [
    {
      "type": "row",
      "id": 1,
      "title": "System Health",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "stat",
      "id": 2,
      "title": "Build Info",
      "gridPos": { "h": 3, "w": 6, "x": 0, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sb_build_info",
          "refId": "A",
          "legendFormat": "{{version}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "value" },
          "noValue": "N/A"
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background",
        "textMode": "name"
      }
    },
    {
      "type": "stat",
      "id": 3,
      "title": "Uptime",
      "gridPos": { "h": 3, "w": 6, "x": 6, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "time() - process_start_time_seconds",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "value" }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 4,
      "title": "Memory Usage",
      "gridPos": { "h": 3, "w": 6, "x": 12, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 524288000, "color": "yellow" },
              { "value": 1073741824, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 5,
      "title": "CPU Usage",
      "gridPos": { "h": 3, "w": 6, "x": 18, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m]) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 60, "color": "yellow" },
              { "value": 80, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "row",
      "id": 10,
      "title": "Traffic Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 4 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 11,
      "title": "Overall QPS (DNS + HTTP + Outbound)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(dns_query_total[1m]))",
          "legendFormat": "DNS Queries",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_respond_total[1m]))",
          "legendFormat": "HTTP Responses",
          "refId": "B"
        },
        {
          "expr": "sum(rate(outbound_connect_total[1m]))",
          "legendFormat": "Outbound Connections",
          "refId": "C"
        },
        {
          "expr": "sum(rate(proxy_select_total[1m]))",
          "legendFormat": "Proxy Selections",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last", "mean", "max"]
        },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "row",
      "id": 20,
      "title": "Success Rates & Health",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "type": "stat",
      "id": 21,
      "title": "DNS Success Rate",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(dns_success_total[5m])) / (sum(rate(dns_success_total[5m])) + sum(rate(dns_failed_total[5m])))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 90, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 22,
      "title": "HTTP Success Rate",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(http_respond_total{code=~\"2..|3..\"}[5m])) / sum(rate(http_respond_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 90, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 23,
      "title": "Outbound Success Rate",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(outbound_connect_total{result=\"success\"}[5m])) / sum(rate(outbound_connect_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 80, "color": "yellow" },
              { "value": 95, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 24,
      "title": "Proxies Up",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "count(proxy_up == 1) OR on() vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "row",
      "id": 30,
      "title": "Resource Utilization",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 20 },
      "collapsed": false
    },
    {
      "type": "stat",
      "id": 31,
      "title": "UDP NAT Sessions",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 21 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "udp_nat_size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 5000, "color": "yellow" },
              { "value": 9000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 32,
      "title": "DNS Cache Size",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 21 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "dns_cache_size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 33,
      "title": "HTTP Active Connections",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 21 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "http_active_connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 34,
      "title": "DNS Cache Hit Rate",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 21 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(dns_cache_hit_total[1m])) / (sum(rate(dns_cache_hit_total[1m])) + sum(rate(dns_cache_miss_total[1m])))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 50, "color": "yellow" },
              { "value": 80, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "row",
      "id": 40,
      "title": "Error Trends",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 25 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 41,
      "title": "Error Rates by Component",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 26 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(dns_error_total[1m]))",
          "legendFormat": "DNS Errors",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_errors_total[1m]))",
          "legendFormat": "HTTP Errors",
          "refId": "B"
        },
        {
          "expr": "sum(rate(outbound_connect_error_total[1m]))",
          "legendFormat": "Outbound Errors",
          "refId": "C"
        },
        {
          "expr": "sum(rate(udp_upstream_fail_total[1m]))",
          "legendFormat": "UDP Failures",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last", "mean", "max"]
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "prometheus"
        }
      }
    ]
  },
  "annotations": {
    "list": []
  }
}
