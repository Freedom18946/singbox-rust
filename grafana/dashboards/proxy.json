{
  "uid": "singbox-proxy",
  "title": "SingBox-Rust Â· Proxy & Outbound",
  "description": "Proxy selection, outbound connections, and health monitoring for singbox-rust",
  "tags": ["singbox-rust", "proxy", "outbound"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "type": "row",
      "id": 1,
      "title": "Proxy Selection",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 2,
      "title": "Selections by Pool/Endpoint",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (pool, endpoint) (rate(proxy_select_total[1m]))",
          "legendFormat": "{{pool}}/{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean", "max"]
        },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "timeseries",
      "id": 3,
      "title": "Endpoint RTT (EMA, ms)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "avg by (pool, endpoint) (proxy_endpoint_rtt_ms)",
          "legendFormat": "{{pool}}/{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 0
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "timeseries",
      "id": 4,
      "title": "Selection Score",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 9 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "avg by (pool, endpoint) (proxy_select_score)",
          "legendFormat": "{{pool}}/{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "row",
      "id": 10,
      "title": "Outbound Connections",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 11,
      "title": "Connection Rate by Kind",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (kind, result) (rate(outbound_connect_total[1m]))",
          "legendFormat": "{{kind}} - {{result}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "cps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "type": "timeseries",
      "id": 12,
      "title": "TCP Connect Duration (Percentiles)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(tcp_connect_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(tcp_connect_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(tcp_connect_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "stat",
      "id": 13,
      "title": "Success Rate (%)",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 26 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(outbound_connect_total{result=\"success\"}[5m])) / sum(rate(outbound_connect_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 80, "color": "yellow" },
              { "value": 95, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "id": 14,
      "title": "Total Connections (1m)",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 26 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(outbound_connect_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "value" },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "stat",
      "id": 15,
      "title": "Errors (1m)",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 30 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(increase(outbound_connect_error_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 50, "color": "red" }
            ]
          },
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "timeseries",
      "id": 16,
      "title": "Error Rate by Class",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 26 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (class) (rate(outbound_connect_error_total[1m]))",
          "legendFormat": "{{class}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "row",
      "id": 20,
      "title": "Health & Circuit Breaking",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 34 },
      "collapsed": false
    },
    {
      "type": "stat",
      "id": 21,
      "title": "Proxy Health (Up/Down)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 35 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "avg by (pool, endpoint) (proxy_up)",
          "legendFormat": "{{pool}}/{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "max": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          },
          "mappings": [
            { "type": "value", "value": 0, "text": "Down" },
            { "type": "value", "value": 1, "text": "Up" }
          ]
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      }
    },
    {
      "type": "timeseries",
      "id": 22,
      "title": "Circuit State Changes",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 35 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (pool, endpoint, state) (rate(proxy_circuit_state_total[1m]))",
          "legendFormat": "{{pool}}/{{endpoint}} - {{state}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["last"]
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "prometheus"
        }
      },
      {
        "name": "pool",
        "type": "query",
        "query": "label_values(proxy_select_total, pool)",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "multi": true,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2
      },
      {
        "name": "endpoint",
        "type": "query",
        "query": "label_values(proxy_select_total{pool=~\"$pool\"}, endpoint)",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "multi": true,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2
      },
      {
        "name": "outbound_kind",
        "type": "query",
        "query": "label_values(outbound_connect_total, kind)",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "multi": true,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2
      }
    ]
  },
  "annotations": {
    "list": []
  }
}
