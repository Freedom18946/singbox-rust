{
  "log": { "level": "info", "timestamp": true },
  "dns": {
    "servers": [
      { "tag": "local", "address": "udp://223.5.5.5", "detour": "direct", "strategy": "ipv4_only" },
      { "tag": "remote", "address": "https://1.1.1.1/dns-query", "detour": "direct", "strategy": "ipv4_only" },
      { "tag": "block", "address": "rcode://refused" }
    ],
    "rules": [
      { "domain_suffix": ["lan"], "server": "local" },
      { "geosite": ["cn"], "server": "local" },
      { "geosite": ["geolocation-!cn"], "server": "remote" }
    ],
    "strategy": "ipv4_only"
  },
  "ntp": { "enabled": false, "server": "time.apple.com", "server_port": 123, "interval": "30m" },
  "certificate": {},
  "endpoints": [],
  "inbounds": [
    { "type": "socks", "tag": "socks-in", "listen": "127.0.0.1", "listen_port": 1080, "users": [] }
  ],
  "outbounds": [
    { "type": "direct", "tag": "direct" },
    { "type": "block", "tag": "block" }
  ],
  "route": {
    "rules": [ { "protocol": ["dns"], "outbound": "direct" } ],
    "final": "direct",
    "auto_detect_interface": true
  },
  "services": [],
  "experimental": {}
}